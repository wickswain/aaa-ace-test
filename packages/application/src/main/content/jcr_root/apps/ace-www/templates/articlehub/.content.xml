<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Template"
    jcr:title="Article Hub Page"
    allowedPaths="[/content/ace-www(/.*)?]"
    ranking="{Long}400">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="aaa-core/components/page/genericpage">
        <content-par
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/foundation/components/parsys">
            <pageintro
                jcr:primaryType="nt:unstructured"
                sling:resourceType="aaa-core/components/content/pageintro"/>
	        <articlehub-featured
	            jcr:primaryType="nt:unstructured"
	            sling:resourceType="aaa-core/components/content/articlehub-featured"/>
	        <article-list
	            jcr:primaryType="nt:unstructured"
	            sling:resourceType="aaa-core/components/content/articlelist"/>
       	</content-par>
   	</jcr:content>
</jcr:root>
