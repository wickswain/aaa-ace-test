<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Global Header"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/tabs"
            type="nav"/>
        <items jcr:primaryType="nt:unstructured">
            <leftsidelink1
                jcr:primaryType="nt:unstructured"
                jcr:title="Left Side Link - 1"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <icon1
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Select small image for icon"
                                fieldLabel="Icon Image"
                                name="./icon1Path"
                                rootPath="/content/dam/"/>
                            <icon1alttext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Icon Alt Text"
                                name="./icon1Alt"/>
                            <linklabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please enter link label."
                                fieldLabel="Link Label"
                                name="./leftLink1Label"/>
                            <linkto
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="If it is a AAA App URL, please enter URL in the format-http://[Domain].[ClubName].aaa.com/home.html"
                                fieldLabel="Link To"
                                name="./leftLink1LinkTo"
                                rootPath="/content"
                                value="http://[Domain].[ClubName].aaa.com/home.html"/>
                            <openinnewtabflag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Please select the check box if the link has to open in new tab."
                                name="./leftLink1NewTabFlag"
                                text="Open in new window?"
                                value="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </leftsidelink1>
            <leftsidelink2
                jcr:primaryType="nt:unstructured"
                jcr:title="Left Side Link - 2"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <linklabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please enter link label."
                                fieldLabel="Link Label"
                                name="./leftLink2Label"/>
                            <linkto
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="If it is a AAA App URL, please enter URL in the format-http://[Domain].[ClubName].aaa.com/home.html"
                                fieldLabel="Link To"
                                name="./leftLink2LinkTo"
                                rootPath="/content"
                                value="http://[Domain].[ClubName].aaa.com/home.html"/>
                            <openinnewtabflag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Please select the check box if the link has to open in new tab."
                                name="./leftLink2NewTabFlag"
                                text="Open in new window?"
                                value="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </leftsidelink2>
            <rightsidelink1
                jcr:primaryType="nt:unstructured"
                jcr:title="Right Side Link - 1"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <linklabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please enter link label."
                                fieldLabel="Link Label"
                                name="./rightLink1Label"/>
                            <linkto
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="If it is a AAA App URL, please enter URL in the format-http://[Domain].[ClubName].aaa.com/home.html"
                                fieldLabel="Link To"
                                name="./rightLink1LinkTo"
                                rootPath="/content"
                                value="http://[Domain].[ClubName].aaa.com/home.html"/>
                            <openinnewtabflag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Please select the check box if the link has to open in new tab."
                                name="./rightLink1NewTabFlag"
                                text="Open in new window?"
                                value="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </rightsidelink1>
            <rightsidelink2
                jcr:primaryType="nt:unstructured"
                jcr:title="Right Side Link - 2"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <icon2
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Select small image for icon"
                                fieldLabel="Icon Image"
                                name="./icon2Path"
                                rootPath="/content/dam/"/>
                            <icon2alttext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Icon Alt Text"
                                name="./icon2Alt"/>
                            <linklabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please enter link label."
                                fieldLabel="Link Label"
                                name="./rightLink2Label"/>
                            <linkto
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="If it is a AAA App URL, please enter URL in the format-http://[Domain].[ClubName].aaa.com/home.html"
                                fieldLabel="Link To"
                                name="./rightLink2LinkTo"
                                rootPath="/content"
                                value="http://[Domain].[ClubName].aaa.com/aceapps/authenticate/login"/>
                            <openinnewtabflag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Please select the check box if the link has to open in new tab."
                                name="./rightLink2NewTabFlag"
                                text="Open in new window?"
                                value="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </rightsidelink2>
            <account
                jcr:primaryType="nt:unstructured"
                jcr:title="Profile Links"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <signoutlinklabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please enter signout link label."
                                fieldLabel="Signout Link Label"
                                name="./signOutLinkLabel"/>
                            <signoutlinkto
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="If it is a AAA App URL, please enter URL in the format-http://[Domain].[ClubName].aaa.com/home.html"
                                fieldLabel="Signout Link To"
                                name="./signOutLinkTo"
                                value="http://[Domain].[ClubName].aaa.com/aceapps/authenticate/logout"/>
                            <defaultavatar
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="Select small image for default avatar icon"
                                fieldLabel="Default Avatar Icon Image"
                                name="./defaultAvatarPath"
                                rootPath="/content/dam/"/>
                            <defaultavataralttext
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Default Avatar Icon Alt Text"
                                name="./defaultAvatarAlt"/>
                            <accountlinklabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please enter signout link label."
                                fieldLabel="MyAccount Link Label"
                                name="./accountLinkLabel"/>
                            <accountlinkto
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="If it is a AAA App URL, please enter URL in the format-http://[Domain].[ClubName].aaa.com/home.html"
                                fieldLabel="MyAccount Link To"
                                name="./accountLinkTo"
                                value="http://[Domain].[ClubName].aaa.com/aceapps/account/my-account"/>
                            <openinnewtabflag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Please select the check box if the account link has to open in new tab."
                                name="./accountLinkNewTabFlag"
                                text="Open in new window?"
                                value="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </account>
            <search
                jcr:primaryType="nt:unstructured"
                jcr:title="Search"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <searchlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Search Label"
                                name="./searchLabel"
                                value="Search"/>
                            <pagepath
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldLabel="Search Page Path"
                                name="./searchPagePath"
                                rootPath="/content/ace-www"
                                value="/content/ace-www/en/search-results.html"/>
                            <placeholder
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldLabel="Placeholder Label"
                                name="./searchPlaceholderLabel"
                                value="Search"/>
                        </items>
                    </columns>
                </items>
            </search>
            <joinbtn
                jcr:primaryType="nt:unstructured"
                jcr:title="Join Button"
                sling:resourceType="granite/ui/components/foundation/container">
                <layout
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
                <items jcr:primaryType="nt:unstructured">
                    <columns
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <buttonlabel
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/textfield"
                                fieldDescription="Please enter the button label."
                                fieldLabel="Button Label"
                                name="./joinButtonLabel"/>
                            <buttonurl
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                                fieldDescription="If it is a AAA App URL, please enter URL in the format-http://[Domain].[ClubName].aaa.com/home.html"
                                fieldLabel="Button URL"
                                name="./joinButtonURL"
                                rootPath="/content"
                                value="http://[Domain].[ClubName].aaa.com/home.html"/>
                            <openinnewtabflag
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/checkbox"
                                fieldDescription="Please select the check box if the link has to open in new tab."
                                name="./joinButtonOpenTabFlag"
                                text="Open in new window?"
                                value="{Boolean}true"/>
                        </items>
                    </columns>
                </items>
            </joinbtn>
        </items>
    </content>
</jcr:root>
