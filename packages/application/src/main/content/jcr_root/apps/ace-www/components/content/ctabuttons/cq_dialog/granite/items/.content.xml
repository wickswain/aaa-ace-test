    jcr:primaryType="nt:unstructured">
    <ctatypetab
        jcr:primaryType="nt:unstructured"
        jcr:title="CTA Type Selector"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <isbutton
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        class="cq-dialog-checkbox-showhide"
                        cq-dialog-checkbox-showhide-target=".button-option-enable-showhide-target"
                        fieldDescription="Please check box to enable button tabs"
                        name="./isButton"
                        text="Enable Button Tabs?"
                        value="{Boolean}true"/>
                    <islink
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        class="cq-dialog-checkbox-showhide"
                        cq-dialog-checkbox-showhide-target=".link-option-enable-showhide-target"
                        fieldDescription="Please check box to enable link tabs"
                        name="./isLink"
                        text="Enable Link Tabs?"
                        value="{Boolean}true"/>
                </items>
            </columns>
        </items>
    </ctatypetab>
    <ctalinktab-1
        jcr:primaryType="nt:unstructured"
        jcr:title="CTA Link-1"
        sling:resourceType="granite/ui/components/foundation/container"
        class="hide link-option-enable-showhide-target"
        showhidetargetvalue="true">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <linkstyle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/select"
                        fieldDescription="Please select Link Style"
                        fieldLabel="Link-1 Style"
                        name="./link1/linkStyle">
                        <items jcr:primaryType="nt:unstructured">
                            <textlinkregular
                                jcr:primaryType="nt:unstructured"
                                text="Text Link Regular"
                                value="textlinkregular"/>
                            <textlinkreversed
                                jcr:primaryType="nt:unstructured"
                                text="TextLink Reversed"
                                value="textlinkreversed"/>
                        </items>
                    </linkstyle>
                    <cta-label
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter label for Button/Link"
                        fieldLabel="Link-1 Label"
                        name="./link1/ctaLabel"
                        required="{Boolean}false"
                        text="Label"/>
                    <url-path
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                        fieldDescription="Browse The Link Path or URL"
                        fieldLabel="Link-1 URL"
                        name="./link1/urlPath"
                        rootPath="/content"
                        showhidetargetvalue="linkstyle"
                        suffix=".html"/>
                    <queryStringKey
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter Query String Key"
                        fieldLabel="Link-1 Query String Key"
                        name="./link1/queryStringKey"/>
                    <phone-dial-flag
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        fieldDescription="Please check box if it is phone number"
                        name="./link1/phonedialFlag"
                        text="Is Phone Number?"
                        value="{Boolean}false"/>
                    <open-in-new-tab-flag
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        fieldDescription="Please check box if it has to open in new Tab"
                        name="./link1/openNewTabFlag"
                        text="Open in new windowr?"
                        value="{Boolean}true"/>
                </items>
            </columns>
        </items>
    </ctalinktab-1>
