<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <ctatypetab
        jcr:primaryType="nt:unstructured"
        jcr:title="CTA Type Selector"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <cta-type
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/radiogroup">
                        <items jcr:primaryType="nt:unstructured">
                            <button
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/radio"
                                class="cq-dialog-checkbox-showhide"
                                cq-dialog-checkbox-showhide-target=".button-option-enable-showhide-target"
                                name="./ctaType"
                                text="Enable Buttons"
                                value="buttons"/>
                            <link
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/foundation/form/radio"
                                class="cq-dialog-checkbox-showhide"
                                cq-dialog-checkbox-showhide-target=".button-option-enable-showhide-target"
                                name="./ctaType"
                                text="Enable Links"
                                value="links"/>
                        </items>
                    </cta-type>
                </items>
            </columns>
        </items>
    </ctatypetab>
    <ctalinktab-1
        jcr:primaryType="nt:unstructured"
        jcr:title="CTA Link-1"
        sling:resourceType="granite/ui/components/foundation/container"
        class="hide button-option-enable-showhide-target"
        showhidetargetvalue="links">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <linkstyle
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/select"
                        fieldDescription="Please select Link Style"
                        fieldLabel="Link-1 Style"
                        name="./link1/linkStyle">
                        <items jcr:primaryType="nt:unstructured">
                            <textlinkregular
                                jcr:primaryType="nt:unstructured"
                                text="Text Link Regular"
                                value="default"/>
                            <textlinkreversed
                                jcr:primaryType="nt:unstructured"
                                text="TextLink Reversed"
                                value="link-reversed"/>
                        </items>
                    </linkstyle>
                    <cta-label
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter label for Button/Link"
                        fieldLabel="Link-1 Label"
                        name="./link1/ctaLabel"
                        required="{Boolean}false"
                        text="Label"/>
                    <cta-color
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="/apps/aaa-core/granite/widgets/colorpicker"
                        colorsPath="/etc/config/aaa-core/components/ctacolorpicker"
                        fieldDescription="Select the container background color"
                        fieldLabel="CTA Color"
                        freestylePalette="{Boolean}true"
                        name="./link1/ctaColor"/>
                    <url-path
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/pathbrowser"
                        fieldDescription="If it is a AAA App URL, please enter URL in the format-http://[Domain].[ClubName].aaa.com/home.html"
                        fieldLabel="Link-1 URL"
                        name="./link1/urlPath"
                        rootPath="/content"
                        showhidetargetvalue="linkstyle"/>
                    <querystringkeyselection
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/select"
                        fieldDescription="Please select a key from list of commonly used query string keys"
                        fieldLabel="Select Query String"
                        name="./link1/selectedQueryString"
                        multiple="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <campaigncode
                                jcr:primaryType="nt:unstructured"
                                text="campaigncode"
                                value="campaigncode"/>
                            <area
                                jcr:primaryType="nt:unstructured"
                                text="area"
                                value="area"/>
                            <intcmp
                                jcr:primaryType="nt:unstructured"
                                text="intcmp"
                                value="intcmp"/>
                            <cid
                                jcr:primaryType="nt:unstructured"
                                text="cid"
                                value="cid"/>
                            <bcd
                                jcr:primaryType="nt:unstructured"
                                text="bcd"
                                value="bcd"/>
                        </items>
                    </querystringkeyselection>
                    <querystringkey
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Please enter custom Query String Key if the query string you want to author is not available in above dropdown, please note that it will affect site performance!"
                        fieldLabel="Link-1 Custom Query String"
                        name="./link1/queryStringKey"/>
                    <phone-dial-flag
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        fieldDescription="Please check box if it is phone number"
                        name="./link1/phonedialFlag"
                        text="Is Phone Number?"
                        value="{Boolean}true"/>
                    <open-in-new-tab-flag
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/checkbox"
                        fieldDescription="Please check box if it has to open in new Tab"
                        name="./link1/openNewTabFlag"
                        text="Open in new window?"
                        value="{Boolean}true"/>
                </items>
            </columns>
        </items>
    </ctalinktab-1>
