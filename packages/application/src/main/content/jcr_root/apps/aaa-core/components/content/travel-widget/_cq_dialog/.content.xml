                                fieldLabel="Select Highlight Tab"
                                name="./tabType">
                                <items jcr:primaryType="nt:unstructured">
                                    <hotels
                                        jcr:primaryType="nt:unstructured"
                                        text="Hotels (Default)"
                                        value="hotels"/>
                                    <flights
                                        jcr:primaryType="nt:unstructured"
                                        text="Flights"
                                        value="flights"/>
                                    <cars
