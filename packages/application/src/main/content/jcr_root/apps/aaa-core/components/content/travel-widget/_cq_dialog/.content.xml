<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Travel Widget"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/foundation/container">
        <layout
            jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <highlighttab
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/select"
                        fieldDescription="Select the tab which is highlighted by default. "
                        fieldLabel="Select Highlight Tab"
                        name="./tabType">
                        <items jcr:primaryType="nt:unstructured">
                            <hotels
                                jcr:primaryType="nt:unstructured"
                                text="Hotels (Default)"
                                value="hotels"/>
                            <flights
                                jcr:primaryType="nt:unstructured"
                                text="Flights"
                                value="flights"/>
                            <cars
                                jcr:primaryType="nt:unstructured"
                                text="Cars"
                                value="cars"/>
                            <cruises
                                jcr:primaryType="nt:unstructured"
                                text="Cruises"
                                value="cruises"/>
                            <vacations
                                jcr:primaryType="nt:unstructured"
                                text="Vacations"
                                value="vacations"/>
                        </items>
                    </highlighttab>
                    <refid
                    	jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        defaultValue="5776"
                        fieldDescription="Reference ids[(CA / 5733),(TX / 5799),(NM / 5748),(HI / 5738),(NNE / 5741),(TW / 5780),(AL / 5755),(ACMO / 5783),(EC / 5770)]. Default value section(5776)"
                        fieldLabel="Enter Reference ID"
                        name="./refId"
                        value="5776"/>
                    <clubcode
                    	jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Club codes[(CA / 004),(TX / 252),(NM / 601),(HI / 018),(NNE / 036),(TW / 258),(AL / 001),(ACMO / 065),(EC / 215)]."
                        fieldLabel="Enter Partner Code"
                        name="./clubCode"/>
                    <partnercode
                    	jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Partner codes[(CA / 53709),(TX / 58592),(NM / 58593),(HI / 60996),(NNE / 67113),(TW / 35190),(AL / 1817),(ACMO / 21533),(EC / 31333)]."
                        fieldLabel="Enter Partner Code"
                        name="./partnerCode"/>
                    <webserialnumber
                    	jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/form/textfield"
                        fieldDescription="Web serial numbers[(CA / 1001),(TX / 1280),(NM / 1281),(HI / 1482),(NNE / 2732),(TW / 1167),(AL / 1116),(ACMO / 1183),(EC / 1198)]."
                        fieldLabel="Enter Web Serial Number"
                        name="./wsnCode"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
